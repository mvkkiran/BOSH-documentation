    <!-- http://purl.org/bosh#Acceleration -->


    <owl:Class rdf:about="http://purl.org/bosh#Acceleration">
        <rdfs:subClassOf rdf:resource="http://purl.org/bosh#QuantityKind"/>
        <rdfs:comment xml:lang="en">The rate of change of the velocity of a particle with respect to time.</rdfs:comment>
        <rdfs:label xml:lang="en">Acceleration</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.oclc.org/NET/ssnx/qu/dim#Acceleration"/>
    </owl:Class>
    


    <!-- http://purl.org/bosh#AccelerationInstantaneous -->


