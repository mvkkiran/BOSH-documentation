    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/bosh#belongsTo -->


    <owl:ObjectProperty rdf:about="http://purl.org/bosh#belongsTo">
        <rdfs:domain rdf:resource="http://purl.org/bosh#Control"/>
        <rdfs:range rdf:resource="http://purl.org/bosh#Device"/>
        <rdfs:range rdf:resource="http://purl.org/bosh#SecuritySystem"/>
        <rdfs:comment xml:lang="en">Property used to map the controls like switches, buttons etc with the devices.</rdfs:comment>
        <rdfs:label xml:lang="en">belongs to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/bosh#contains -->


    <owl:ObjectProperty rdf:about="http://purl.org/bosh#contains">
        <rdfs:domain rdf:resource="http://purl.org/bosh#BuildingEnvironment"/>
        <rdfs:range rdf:resource="http://purl.org/bosh#Appliance"/>
        <rdfs:range rdf:resource="http://purl.org/bosh#BuildingAutomation"/>
        <rdfs:range rdf:resource="http://purl.org/bosh#BuildingEnvironment"/>
        <rdfs:range rdf:resource="http://purl.org/bosh#Device"/>
        <rdfs:range rdf:resource="http://purl.org/bosh#SecuritySystem"/>
        <rdfs:comment xml:lang="en">This property is used to map all building related items.</rdfs:comment>
        <rdfs:label xml:lang="en">contains</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/bosh#hasActuator -->


    <owl:ObjectProperty rdf:about="http://purl.org/bosh#hasActuator">
        <rdfs:domain rdf:resource="http://purl.org/bosh#Appliance"/>
        <rdfs:domain rdf:resource="http://purl.org/bosh#BuildingAutomation"/>
        <rdfs:range rdf:resource="http://purl.org/bosh#ActuatingDevice"/>
        <rdfs:comment xml:lang="en">Property used to map actuators with the respective classes.</rdfs:comment>
        <rdfs:label xml:lang="en">has Actuator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/bosh#hasDomainOfInterest -->


    <owl:ObjectProperty rdf:about="http://purl.org/bosh#hasDomainOfInterest">
        <rdfs:range rdf:resource="http://purl.org/bosh#ApplicationDomain"/>
        <rdfs:comment xml:lang="en">This property is used to classify devices by DomainOfInterest (e.g., blood pressure sensor is used in healthcare).</rdfs:comment>
        <rdfs:label xml:lang="en">has Domain Of Interest</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/bosh#hasMeasurementType -->


    <owl:ObjectProperty rdf:about="http://purl.org/bosh#hasMeasurementType">
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/bosh#hasMeasurementType"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://purl.org/bosh#MeasurementType"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Each sensing device can have a different sensing mechanism which may result in different kinds of sensor data. This property links observation of the sensor to the associated MeasurementType.</rdfs:comment>
        <rdfs:label xml:lang="en">has Measurement Type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/bosh#hasSensor -->


    <owl:ObjectProperty rdf:about="http://purl.org/bosh#hasSensor">
        <rdfs:domain rdf:resource="http://purl.org/bosh#Appliance"/>
        <rdfs:domain rdf:resource="http://purl.org/bosh#BuildingAutomation"/>
        <rdfs:range rdf:resource="http://purl.org/bosh#SensingDevice"/>
        <rdfs:comment xml:lang="en">Property used to map sensors with the respective classes.</rdfs:comment>
        <rdfs:label xml:lang="en">has Sensor</rdfs:label>
    </owl:ObjectProperty>
    


