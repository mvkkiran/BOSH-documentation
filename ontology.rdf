     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://purl.org/bosh">
        <terms:created>2021-07-14</terms:created>
        <terms:creator xml:lang="en">Amelie Gyrard</terms:creator>
        <terms:creator xml:lang="en">Mahda Noura</terms:creator>
        <terms:creator xml:lang="en">Ramesh Gowtham</terms:creator>
        <terms:creator xml:lang="en">Vamsee Krishna Kiran Muppavarapu</terms:creator>
        <terms:description xml:lang="en">Ontology covering the concepts related to smart home and smart building domains. Ontology contains sensors, actuators, building entities, measurements and so on.</terms:description>
        <terms:modified>2021-11-30</terms:modified>
        <terms:title xml:lang="en">Smart Home and Building Ontology (BOSH)</terms:title>
        <rdfs:comment xml:lang="en">Smart Home and Building Upper Ontology developed as part of PhD research work.</rdfs:comment>
    </owl:Ontology>
    


