        <rdfs:subClassOf rdf:resource="http://purl.org/bosh#Acceleration"/>
        <rdfs:comment xml:lang="en">The acceleration at a given instant of time.</rdfs:comment>
        <rdfs:label xml:lang="en">Instantaneous Acceleration</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/bosh#Accelerometer -->


    <owl:Class rdf:about="http://purl.org/bosh#Accelerometer">
        <owl:equivalentClass rdf:resource="http://purl.org/iot/vocab/m3-lite#Accelerometer"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/bosh#SensingDevice"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/bosh#hasDomainOfInterest"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/bosh#BuildingAutomation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"> Accelerometers are used to automatically determine the orientation in which the user is holding the IoT Object (portrait or landscape)</rdfs:comment>
        <rdfs:label xml:lang="en">Accelerometer</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://casas.wsu.edu/owl/cose.owl#Accelerometer"/>
    </owl:Class>
    


    <!-- http://purl.org/bosh#AccessControlSystem -->


    <owl:Class rdf:about="http://purl.org/bosh#AccessControlSystem">
        <rdfs:subClassOf rdf:resource="http://purl.org/bosh#SecuritySystem"/>
        <rdfs:comment xml:lang="en">System that controls access to certain devices, environment or places.</rdfs:comment>
        <rdfs:label xml:lang="en">Access Control System</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/bosh#ActuatingDevice -->


    <owl:Class rdf:about="http://purl.org/bosh#ActuatingDevice">
        <owl:equivalentClass rdf:resource="https://w3id.org/saref#Actuator"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/bosh#Device"/>
        <rdfs:comment xml:lang="en">Device that can actuate over an object or QuantityKind (e.g., switch on/off the light).</rdfs:comment>
        <rdfs:label xml:lang="en">Actuating Device</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/bosh#AirConditioner -->


    <owl:Class rdf:about="http://purl.org/bosh#AirConditioner">
        <owl:equivalentClass rdf:resource="http://purl.org/iot/vocab/m3-lite#AirConditioner"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/bosh#ActuatingDevice"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/bosh#AirConditioningSystem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/bosh#hasDomainOfInterest"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/bosh#BuildingAutomation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An actuator to automatically switch on/off the air conditioner.</rdfs:comment>
        <rdfs:label xml:lang="en">Air Conditioner</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/bosh#AirConditioningSystem -->


    <owl:Class rdf:about="http://purl.org/bosh#AirConditioningSystem">
        <rdfs:subClassOf rdf:resource="http://purl.org/bosh#ActuatingDevice"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/bosh#HVAC"/>
        <rdfs:comment xml:lang="en">A conditioning system/utility to keep a room/building cool or dry.</rdfs:comment>
        <rdfs:label xml:lang="en">Air Conditioning System</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/bosh#AirFlowSensor -->


    <owl:Class rdf:about="http://purl.org/bosh#AirFlowSensor">
        <rdfs:subClassOf rdf:resource="http://purl.org/bosh#SensingDevice"/>
        <rdfs:comment xml:lang="en">Device used to detect the Air Flow.</rdfs:comment>
        <rdfs:label xml:lang="en">Air Flow Sensor</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/bosh#AirHumiditySensor -->


    <owl:Class rdf:about="http://purl.org/bosh#AirHumiditySensor">
        <rdfs:subClassOf rdf:resource="http://purl.org/bosh#HumiditySensor"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/bosh#hasDomainOfInterest"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/bosh#Environment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Sensor used to measure air humidity.</rdfs:comment>
        <rdfs:label xml:lang="en">Air Humidity Sensor</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/bosh#AirPollution -->


    <owl:Class rdf:about="http://purl.org/bosh#AirPollution">
        <rdfs:subClassOf rdf:resource="http://purl.org/bosh#QuantityKind"/>
        <rdfs:comment xml:lang="en">Usually measured using Air Quality Index (AQI), it is the measure of Air Pollution in the environment. It is similar to Air Quality.</rdfs:comment>
        <rdfs:label xml:lang="en">Air Pollution Quantity Kind</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/bosh#AirPollutionSensor -->


    <owl:Class rdf:about="http://purl.org/bosh#AirPollutionSensor">
        <owl:equivalentClass rdf:resource="http://purl.org/iot/vocab/m3-lite#AirPollutantSensor"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/bosh#SensingDevice"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/bosh#hasDomainOfInterest"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/bosh#Environment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Air Pollution Sensor are devices that detect and monitor the presence of air pollution in the surrounding area.</rdfs:comment>
        <rdfs:label xml:lang="en">Air Pollution Sensor</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/bosh#AirQuality -->


    <owl:Class rdf:about="http://purl.org/bosh#AirQuality">
        <rdfs:subClassOf rdf:resource="http://purl.org/bosh#QuantityKind"/>
        <rdfs:comment xml:lang="en">Usually measured using an air quality index (AQI) that is a number used by government agencies to communicate to the public how polluted the air currently is or how polluted it is forecast to become. Different countries have their own air quality indices, corresponding to different national air quality standards. It is the measure of Air Quality of the environment. It is similar to Air Pollution. (Source Wikipedia).</rdfs:comment>
        <rdfs:label xml:lang="en">Air Quality Quantity Kind</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/bosh#AirTemperature -->


    <owl:Class rdf:about="http://purl.org/bosh#AirTemperature">
        <rdfs:subClassOf rdf:resource="http://purl.org/bosh#Temperature"/>
        <rdfs:comment xml:lang="en">The temperature of the air that would be indicated by a thermometer exposed to the air at a location sheltered from direct solar radiation.</rdfs:comment>
        <rdfs:label xml:lang="en">Air Temperature, Weather Temperature, Ambient Temperature</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://openiot.eu/ontology/ns/AirTemperature"/>
        <rdfs:seeAlso rdf:resource="http://paul.staroch.name/thesis/SmartHomeWeather.owl#Temperature"/>
    </owl:Class>
    


    <!-- http://purl.org/bosh#AirThermometer -->


    <owl:Class rdf:about="http://purl.org/bosh#AirThermometer">
        <rdfs:subClassOf rdf:resource="http://purl.org/bosh#Thermometer"/>
        <rdfs:comment xml:lang="en">Device to measure the Air temperature of either indoor or outdoor atmosphere.</rdfs:comment>
        <rdfs:label xml:lang="en">Air Thermometer</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/bosh#Alarm -->


    <owl:Class rdf:about="http://purl.org/bosh#Alarm">
        <owl:equivalentClass rdf:resource="http://purl.org/bosh#Siren"/>
        <owl:equivalentClass rdf:resource="http://purl.org/iot/vocab/m3-lite#AlarmSystem"/>
        <rdfs:subClassOf rdf:resource="http://purl.org/bosh#ActuatingDevice"/>
        <rdfs:comment xml:lang="en">An actuator to automatically switch on/off the alarm system.</rdfs:comment>
