#################################################################
#    Object Properties
#################################################################

###  http://purl.org/bosh#belongsTo
:belongsTo rdf:type owl:ObjectProperty ;
           rdfs:domain :Control ;
           rdfs:range :Device ,
                      :SecuritySystem ;
           rdfs:comment "Property used to map the controls like switches, buttons etc with the devices."@en ;
           rdfs:label "belongs to"@en .


###  http://purl.org/bosh#contains
:contains rdf:type owl:ObjectProperty ;
          rdfs:domain :BuildingEnvironment ;
          rdfs:range :Appliance ,
                     :BuildingAutomation ,
                     :BuildingEnvironment ,
                     :Device ,
                     :SecuritySystem ;
          rdfs:comment "This property is used to map all building related items."@en ;
          rdfs:label "contains"@en .


###  http://purl.org/bosh#hasActuator
:hasActuator rdf:type owl:ObjectProperty ;
             rdfs:domain :Appliance ,
                         :BuildingAutomation ;
             rdfs:range :ActuatingDevice ;
             rdfs:comment "Property used to map actuators with the respective classes."@en ;
             rdfs:label "has Actuator"@en .


###  http://purl.org/bosh#hasDomainOfInterest
:hasDomainOfInterest rdf:type owl:ObjectProperty ;
                     rdfs:range :ApplicationDomain ;
                     rdfs:comment "This property is used to classify devices by DomainOfInterest (e.g., blood pressure sensor is used in healthcare)."@en ;
                     rdfs:label "has Domain Of Interest"@en .


###  http://purl.org/bosh#hasMeasurementType
:hasMeasurementType rdf:type owl:ObjectProperty ;
                    rdfs:range [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasMeasurementType ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass :MeasurementType
                               ] ;
                    rdfs:comment "Each sensing device can have a different sensing mechanism which may result in different kinds of sensor data. This property links observation of the sensor to the associated MeasurementType."@en ;
                    rdfs:label "has Measurement Type"@en .


###  http://purl.org/bosh#hasSensor
:hasSensor rdf:type owl:ObjectProperty ;
           rdfs:domain :Appliance ,
                       :BuildingAutomation ;
           rdfs:range :SensingDevice ;
           rdfs:comment "Property used to map sensors with the respective classes."@en ;
           rdfs:label "has Sensor"@en .


